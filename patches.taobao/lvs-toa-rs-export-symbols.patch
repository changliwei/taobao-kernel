+++ linux-2.6.32-220.2.1.el5/include/net/inet_common.h	2012-02-17 13:17:18.996644302 +0800
@@ -1,7 +1,7 @@
 #ifndef _INET_COMMON_H
 #define _INET_COMMON_H
 
-extern const struct proto_ops		inet_stream_ops;
